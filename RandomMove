MÉTODO RANDOM MOVE

public void randomMov(){
        switch (player.getMoveSet()){
            case 0:
                left.setKey(39);
                right.setKey(37);
                up.setKey(40);
                down.setKey(38);
                player.setMoveSet(1);
                break;
            case 1:
                left.setKey(38);
                right.setKey(40);
                up.setKey(39);
                down.setKey(37);
                player.setMoveSet(2);
                break;
            case 2:
                left.setKey(40);
                right.setKey(38);
                up.setKey(37);
                down.setKey(39);
                player.setMoveSet(0);
                break;
        }
        
    PROPRIEDADES À INSERIR NO PLAYER
        
    private int moveCounter;
    private int moveSet;
    
    INCREMENTAR O COUNTER À CADA MOVE
    
    public void moveUp() {
        this.moveCounter += 1;
        this.face.translate(0, -10);
        this.rectangle.translate(0, -10);
    }

    public void moveDown() {
        this.moveCounter += 1;
        this.face.translate(0, 10);
        this.rectangle.translate(0, 10);
    }

    public void moveRight() {
        this.moveCounter += 1;
        this.face.translate(10, 0);
        this.rectangle.translate(10, 0);
    }

    public void moveLeft(){
        this.moveCounter += 1;
        this.face.translate(-10, 0);
        this.rectangle.translate(-10, 0);
    }
    
    GETTERS E SETTERS
    
    public int getMoveCounter(){
        return  this.moveCounter;
    }

    public int getMoveSet(){
        return this.moveSet;
    }

    public void setMoveSet(int moveSet){
       this.moveSet = moveSet;
    }

    public void setMoveCounter(int num){
        this.moveCounter = num;
    }
    
    MODIFICAÇÕES NECESSÁRIAS NO MOVING
    
    if (keyboardEvent == down && movable.getY() < 435) {
            if (player.getIsHigh() && player.getMoveCounter() == 10) {
                randomMov();
                player.setMoveCounter(0);
            } else {
                player.moveDown();
                currentMove = 0;
            }
        }

        if (keyboardEvent == up && movable.getY() > 30) {
            if (player.getIsHigh() && player.getMoveCounter() == 10) {
                randomMov();
                player.setMoveCounter(0);
            } else {
                player.moveUp();
                currentMove = 1;
            }
        }

        if (keyboardEvent == right && movable.getX() < 435) {
            if (player.getIsHigh() && player.getMoveCounter() == 10) {
                randomMov();
                player.setMoveCounter(0);
            } else {
                player.moveRight();
                currentMove = 2;
            }
        }

        if (keyboardEvent == left && movable.getX() > 30) {
            if (player.getIsHigh() && player.getMoveCounter() == 10){
                randomMov();
                player.setMoveCounter(0);
            } else {
                player.moveLeft();
                currentMove = 3;
            }
        }
